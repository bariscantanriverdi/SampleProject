<!DOCTYPE html>
<html>
<head>
    <style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>

<h2>Büşra Barış Table</h2>

<table>
    <tr>
        <th>Ürün Adı</th>
        <th>Fiyat</th>
        <th>Kategori Id</th>
    </tr>
    @foreach (var product in Model)
    {
        <tr>
            <td>@product.Name</td>
            <td>@product.UnitPrice</td>
            <td>@product.CategoryId</td>
                
        </tr>
    }
</table>

</body>
</html>